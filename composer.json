    "keywords": ["aws", "s3", "cloud", "document-storage", "echosign"],
    "homepage": "https://github.com/ETSGlobal/document-storage",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "ETS Global Dev Team",
            "email": "ecs@etsglobal.org"
        }
    ],
    "autoload": {
        "psr-4": { "ETS\\DocumentStorage\\": "src/" }
